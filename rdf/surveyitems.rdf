        <rdawd:P10088>The counselor</rdawd:P10088>
        <rdfs:label>The counselor (McCarthy, Cormac)</rdfs:label>
        <rdaxd:P00029>Screenplay</rdaxd:P00029>
        <rdawo:P10061 rdf:resource="http://viaf.org/viaf/29558386"/>
